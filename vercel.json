{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/products",
      "dest": "api/index.py"
    },
    {
      "src": "/api/bot",
      "dest": "api/bot.py"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
